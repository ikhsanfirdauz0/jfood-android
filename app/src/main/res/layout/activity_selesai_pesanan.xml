<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".SelesaiPesananActivity">

    <TextView
        android:id="@+id/static_invoice_cutomer_name"
        android:layout_width="126dp"
        android:layout_height="26dp"
        android:layout_marginBottom="44dp"
        android:text="TextView"
        android:textColor="#A3A3A3"
        android:textSize="12sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.157"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/static_invoice"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/static_invoice_food_category"
        android:layout_width="96dp"
        android:layout_height="22dp"
        android:layout_marginEnd="116dp"
        android:layout_marginBottom="32dp"
        android:text="Food Category"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.231"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/static_invoice"
        app:layout_constraintVertical_bias="0.272" />

    <TextView
        android:id="@+id/invoice_food_category"
        android:layout_width="77dp"
        android:layout_height="23dp"
        android:text="TextView"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.886"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/static_invoice"
        app:layout_constraintVertical_bias="0.259" />

    <TextView
        android:id="@+id/static_invoice_food_name"
        android:layout_width="67dp"
        android:layout_height="22dp"
        android:text="Food Name"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.133"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/static_invoice_cutomer_name"
        app:layout_constraintVertical_bias="0.164" />

    <TextView
        android:id="@+id/invoice_food_name"
        android:layout_width="77dp"
        android:layout_height="23dp"
        android:layout_marginBottom="44dp"
        android:text="TextView"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.886"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/static_invoice"
        app:layout_constraintVertical_bias="0.175" />

    <TextView
        android:id="@+id/static_invoice"
        android:layout_width="95dp"
        android:layout_height="33dp"
        android:text="Invoice"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.142"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.038" />

    <Button
        android:id="@+id/invoice_button_cancel"
        android:layout_width="225dp"
        android:layout_height="51dp"
        android:background="#FFFFFF"
        android:text="CANCEL"
        android:textColor="#66BB6A"
        android:textSize="14sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.489"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.77" />

    <TextView
        android:id="@+id/invoice_total_price"
        android:layout_width="71dp"
        android:layout_height="23dp"
        android:text="TextView"
        android:textColor="#4281F5"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.726"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.646" />

    <TextView
        android:id="@+id/static_invoice_payment_type"
        android:layout_width="96dp"
        android:layout_height="22dp"
        android:layout_marginStart="44dp"
        android:text="Payment Type"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/invoice_payment_type"
        app:layout_constraintHorizontal_bias="0.336"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.571" />

    <TextView
        android:id="@+id/static_invoice_total_price"
        android:layout_width="96dp"
        android:layout_height="22dp"
        android:layout_marginStart="44dp"
        android:text="Total Price"
        android:textColor="#4281F5"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/invoice_total_price"
        app:layout_constraintHorizontal_bias="0.336"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.647" />

    <TextView
        android:id="@+id/static_invoice_delivery_fee"
        android:layout_width="96dp"
        android:layout_height="22dp"
        android:layout_marginStart="60dp"
        android:text="Delivery Fee"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/invoice_delivery_fee"
        app:layout_constraintHorizontal_bias="0.219"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.499" />

    <TextView
        android:id="@+id/invoice_payment_type"
        android:layout_width="71dp"
        android:layout_height="23dp"
        android:text="TextView"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.726"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.57" />

    <TextView
        android:id="@+id/invoice_delivery_fee"
        android:layout_width="71dp"
        android:layout_height="23dp"
        android:text="TextView"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.726"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/invoice_button_finish"
        android:layout_width="225dp"
        android:layout_height="51dp"
        android:background="#66BB6A"
        android:text="FINISH"
        android:textColor="#ECECEC"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/invoice_button_cancel"
        app:layout_constraintVertical_bias="0.411" />

    <TextView
        android:id="@+id/static_invoice_id"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="ID Invoice"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.128"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/static_invoice"
        app:layout_constraintVertical_bias="0.075" />

    <TextView
        android:id="@+id/invoice_id"
        android:layout_width="75dp"
        android:layout_height="22dp"
        android:text="TextView"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.88"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/static_invoice"
        app:layout_constraintVertical_bias="0.075" />

    <TextView
        android:id="@+id/static_invoice_date"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Date"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.209"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/static_invoice"
        app:layout_constraintVertical_bias="0.363" />

    <TextView
        android:id="@+id/invoice_date"
        android:layout_width="69dp"
        android:layout_height="27dp"
        android:text="TextView"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.722"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/static_invoice"
        app:layout_constraintVertical_bias="0.368" />

    <TextView
        android:id="@+id/static_invoice_no_order"
        android:layout_width="338dp"
        android:layout_height="25dp"
        android:text="There is no order from you yet"
        android:textAlignment="center"
        android:textColor="#C3C3C3"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.493"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.466" />

</androidx.constraintlayout.widget.ConstraintLayout>